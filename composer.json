{
    "name": "genealabs/laravel-model-caching",
    "description": "Automatic caching for Eloquent models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "fico7489/laravel-pivot": "*",
        "illuminate/cache": "5.4 - 5.7",
        "illuminate/config": "5.4 - 5.7",
        "illuminate/console": "5.4 - 5.7",
        "illuminate/database": "5.4 - 5.7",
        "illuminate/support": "5.4 - 5.7",
        "php": ">=7.1.3"
    },
    "require-dev": {
        "doctrine/dbal": "^2.10",
        "fzaninotto/faker": "^1.9",
        "laravel/nova": "^3.0",
        "orchestra/testbench-browser-kit": "^5.0",
        "orchestra/testbench": "^5.0",
        "php-coveralls/php-coveralls" : "^2.2",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.0",
        "sebastian/phpcpd": "^5.0",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/thanks": "^1.2",
        "kalnoy/nestedset": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/factories"
        ],
        "psr-4": {
            "GeneaLabs\\LaravelModelCaching\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelModelCaching\\Providers\\Service"
            ]
        }
    }
}
